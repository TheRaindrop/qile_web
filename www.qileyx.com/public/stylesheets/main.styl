
vendor(prop, args)
    -webkit-{prop} args
    -moz-{prop} args
    {prop} args
    
box-shadow()
    vendor('box-shadow', arguments)
*
    margin 0
    padding 0
    outline none
    &:active, &:focus
        outline none
.disnone 
    display none
    
html
    height 100%
body
    position relative
    font-family '微软雅黑', '宋体'
    font-size 12px
    background-color #fff
    color #ccc
    height 100%
    min-width 1120px
    overflow-x hidden
img
    border none
.mouse  
    cursor pointer
a
    text-decoration none
    color #000
    &:hover
        color #2b95ca
.ihead a, .ifooter a
    color #fff
    &:hover
        color #2b95ca
ul
    list-style none
.ifooter
    background-color #181818
    height 117px
    // padding-top 3px
    // bottom 0
    width 100%
    .icontent
        position relative
        width:1120px
        margin 0 auto
        padding-top 20px
        .logo
            margin-top 20px
        .wen
            margin-left 30px
span.logo,a.wen
    float left
ul.menu-box, div.copy-right, icp.left, p.left
    margin-left 300px
    margin-top 5px
.menu-list
    display inline

